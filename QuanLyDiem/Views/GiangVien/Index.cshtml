@model IEnumerable<QuanLyDiem.Models.GiangVien>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenGiangVien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GioiTinh)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgaySinh)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Khoa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChuyenNganh)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenGiangVien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GioiTinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NgaySinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Khoa.MaKhoa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChuyenNganh.MaChuyenNganh)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaGiangVien">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaGiangVien">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaGiangVien">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
