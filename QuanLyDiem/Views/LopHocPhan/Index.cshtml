@model IEnumerable<QuanLyDiem.Models.LopHocPhan>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenLopHocPhan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HocPhan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GiangVien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HocKy)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenLopHocPhan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HocPhan.MaHocPhan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GiangVien.MaGiangVien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HocKy.MaHocKy)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaLopHocPhan">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaLopHocPhan">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaLopHocPhan">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
